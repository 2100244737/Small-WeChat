/* pages/history/history.wxss */
.history {
    padding: 10px;
    padding-top: 0;
}
.history .item {
    margin-top: 10px;
    display: flex;
}
.history .item .img {
    display: block;
    width: 226rpx;
    height: 260rpx;
}
.history .item .item-right {
    flex:1;
    margin-left: 10px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.item-right  .top .title {
    margin-bottom: 20px;
    font-size: 16px;
    color:#333;
}